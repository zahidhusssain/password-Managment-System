<%-include('header')%>
    <div class="container">
      <h1>Password Category List's</h1>
   <div class="table-responsive">
    <table class="table table-striped table-primary">
      <thead>
        <tr>
          <th>Password Category Name</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
      <% if(records.length > 0){ 
        records.forEach(function(row){
          %>
      <tr>
          <td><%= row.password_category   %></td>
          <td><a href="/passCategory/edit/ <%= row._id   %> "class="btn btn-primary btn-xs"><i class="fa fa-edit"></i> </a>
            <a href="/passCategory/delete/ <%= row._id   %>"class="btn btn-danger btn-xs"><i class="fa fa-trash"></i> </a>
          </td>
        </tr>
          <%}) }%>
      </tbody>
    </table>
   </div>
 </div>
 <%-include('footer')%>